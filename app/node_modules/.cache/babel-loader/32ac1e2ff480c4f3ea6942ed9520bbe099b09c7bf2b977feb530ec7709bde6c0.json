e.password_confirmation.isBlank = this.user.password_confirmation == '';\r\n      console.log(axios.defaults.baseURL)\r\n      if(this.validate.name.isBlank || this.validate.name.isErrorLength || this.validate.email.isBlank || this.validate.email.isInValid || this.validate.password.isBlank || this.validate.password.isErrorLength || this.validate.password.isNotEquals || this.validate.password_confirmation.isBlank)\r\n      {\r\n        this.$re\r\n        return;\r\n      }\r\n      \r\n      axios.post('/register',this.user)\r\n      .then((response) => {\r\n        console.log(response);\r\n       this.status.success.status = true;\r\n        this.status.error.status = false;\r\n      })\r\n      .catch((error) => {\r\n        console.log(error);\r\n        this.status.success.status = false;\r\n        this.status.error.status = true;\r\n      })\r\n    },\r\n    },\r\n};\r\n</script>\r\n<style scoped>\r\n.title{\r\n    font-size: 1.5rem;\r\n  }\r\n  small{\r\n    display: flex;\r\n    color: red;\r\n  }\r\n  .error{\r\n    border : 1px solid red;\r\n  \r\n  }\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}