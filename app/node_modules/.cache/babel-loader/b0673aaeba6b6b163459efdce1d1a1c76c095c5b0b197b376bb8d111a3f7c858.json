            this.showSpinner = true;\r\n                setTimeout(() => {\r\n                    this.$router.push(\"/home\");\r\n                }, 2000);\r\n            })\r\n            .catch(err => {\r\n                console.log(err);\r\n            })\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style scoped>\r\n.divider:after,\r\n.divider:before {\r\n  content: \"\";\r\n  flex: 1;\r\n  height: 1px;\r\n  background: #eee;\r\n}\r\n.h-custom {\r\n  height: calc(100% - 73px);\r\n}\r\n@media (max-width: 450px) {\r\n  .h-custom {\r\n    height: 100%;\r\n  }\r\n}\r\n.loading {\r\n    background: rgba(0, 0, 0, .5);\r\n    width: 100%;\r\n    height: 100%;\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    z-index: 999;\r\n}\r\n\r\n.spinner-center {\r\n    top: 50%;\r\n    left: 50%;\r\n    position: absolute;\r\n}\r\n.title{\r\n  font-size: 1.5rem;\r\n}\r\nsmall{\r\n  display: flex;\r\n  color: red;\r\n}\r\n.error{\r\n  border : 1px solid red;\r\n\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}